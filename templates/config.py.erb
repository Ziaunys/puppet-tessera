DEBUG = <%= scope['tessera::debug'] %>
SECRET_KEY = <%= scope['tessera::secret_key'] %>
DEFAULT_THEME = <%= scope['tessera::default_theme'] %>
DASHBOARD_APPNAME = <%= scope['tessera::dashboard_appname'] %>
SQLALCHEMY_DATABASE_URI = <%= scope['tessera::sqlalchemy_db_uri'] %>
GRAPHITE_URL = <%= scope['tessera::graphite_url'] %>
DISPLAY_TIMEZONE = <%= scope['tessera::display_timezone'] %>
INTERACTIVE_CHARTS_DEFAULT = <%= scope['tessera::interactive_charts_default'] %>
INTERACTIVE_CHARTS_RENDERER <%= scope['tessera::interactive_charts_renderer'] %>
DASHBOARD_RANGE_PICKER = [
<% scope['tessera::dashboard_range_picker'].each do | k, v | -%>
    (<%= k %>, <%= v %>),
<% end -%>
]

DASHBOARD_REFRESH_INTERVALS = [
<% scope['tessera::dashboard_refresh_intervals'].each do | k, v | -%>
    (<%= k %>, <%= v %>),
<% end -%>
]

DEFAULT_REFRESH_INTERVAL = <%= scope['tessera::default_refresh_interval'] %>
